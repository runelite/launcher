{
    "platform": "linux64",
    "jdk": "linux-jdk",
    "executable": "RuneLite",
    "classpath": [
        "target/RuneLite.jar"
    ],
    "mainclass": "net.runelite.launcher.Launcher",
    "vmargs": [
       "-Drunelite.launcher.reflect=true",
       "-Xmx512m",
       "-Xss2m",
       "-XX:CompileThreshold=1500"
    ],
    "output": "native-linux-x86_64/RuneLite.AppDir/",
    "resources": [
      "target/filtered-resources/runelite.desktop",
      "appimage/runelite.png"
    ]
}
